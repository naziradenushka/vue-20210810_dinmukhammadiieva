{
  "name": "tasks-vue-3",
  "version": "5.0.0",
  "private": true,
  "description": "Taskbook for Vue 3 Course on learn.javascript.ru",
  "author": "Grigorii K. Shartsev <g.shartsev@javascript.info>",
  "scripts": {
    "serve": "vite --port 8080",
    "dev": "vite --port 8080",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "file-serve": "serve",
    "taskdev": "cross-env TASK_DEV=1 npm run serve",
    "taskdev:solution": "cross-env TASK_DEV=1 SOLUTION=1 npm run serve",
    "test:solution": "cross-env SOLUTION=1 npm test",
    "test:watch:solution": "cross-env SOLUTION=1 npm run test:watch"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.0",
    "dequal": "^2.0.2",
    "klona": "^2.0.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "luxon": "^2.3.1",
    "moment": "^2.29.2",
    "nanoid": "^3.3.3",
    "pinia": "^2.0.13",
    "uuid": "^8.3.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/babel-preset-app": "^5.0.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.19",
    "@vue/tsconfig": "^0.1.3",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "flush-promises": "^1.0.2",
    "jest": "^26.6.3",
    "jest-date-mock": "^1.0.8",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.5",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.33.9"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": " ^14.15.0 || ^16.0.0"
  }
}
